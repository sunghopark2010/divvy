-- age
select (2013 - birthyear) / 10, count(trip_id) from trips
group by 1 order by 1 asc;
/*
1;1818
2;128455
3;145378
4;74659
5;40936
6;10604
7;839
8;60
9;16
10;144
;356879
*/

-- distance
select
	dbs.distance::int
	, count(t.trip_id)
from trips t left join distances_between_stations dbs on dbs.from_station_id = t.from_station_id and dbs.to_station_id = t.to_station_id
group by 1 order by 1 asc;
/*
0;89545
1;338877
2;181293
3;83956
4;36332
5;14329
6;5627
7;4150
8;2283
9;1849
10;859
11;369
12;189
13;84
14;32
15;7
16;7
*/

