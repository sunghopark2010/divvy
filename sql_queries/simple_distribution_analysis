-- speed
select
	((dbs.distance / t.duration * 3600))::int as velocity
	, count(t.trip_id)
from trips t left join distances_between_stations dbs on dbs.from_station_id = t.from_station_id and dbs.to_station_id = t.to_station_id
group by 1 order by 1 asc;
